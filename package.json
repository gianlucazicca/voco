    "postgres": "^3.4.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3",
    "zod": "^3.22.4"
  },
  "packageManager": "pnpm@9.1.4",
  "devDependencies": {
    "@nuxt/ui": "^2.17.0",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "prisma": "^5.16.1",
    "tailwindcss": "^3.4.4"
